{
    "name": "prewk/serialized-to-ast",
    "description": "Parses serialized PHP things to an AST",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Thornblad",
            "email": "oskar.thornblad@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Prewk\\": "src/"
        }
